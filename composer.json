{
    "name": "codebooth/laravel-query-filters",
    "description": "Filter models the Eloquent way",
    "keywords": [
        "query-filters",
        "query-builder",
        "eloquent",
        "laravel",
        "codebooth"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "illuminate/database": "~5.6.34|~5.7.0|~5.8.0",
        "illuminate/support": "~5.6.34|~5.7.0|~5.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "CodeBooth\\QueryFilters\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeBooth\\QueryFilters\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
